#!/bin/bash
#Check if CFILE variable is set
if [ -z "$CFILE" ]; then
echo "CFILE variable is not set."
exit 1
fi
#Extract the filename without extension
FILENAME=$(basename "$CFILE" .c)
#Compile the C file to generate the assembly code
gcc -S -masm=intel "$CFILE" -o "$FILENAME.s"
#Check if compilation was successful
if [ $? -eq 0 ]; then
echo "Assembly code generated successfully."
else
echo "Error generating assembly code."
fi
